{
  "name": "image-processing-api",
  "version": "1.0.0",
  "description": "Image Processing API built TS and Express",
  "main": "dist/server.js",
  "scripts": {
    "init": "npm install",
    "lint": "npx eslint src/**",
    "prettier": "prettier src/** --write",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "clean": "rm -rf dist",
    "test-smoke": "npm run clean && npm run build && jasmine 'dist/tests/smoke/*.test.js' --fail-fast",
    "test": "npm run clean && npm run build && jasmine",
    "start-dev": "nodemon src/server.ts",
    "start": "npm run clean && npm run build && node dist/."
  },
  "keywords": [
    "image",
    "api"
  ],
  "author": "Biraj Poddar",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "type-fest": "^2.16.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "sharp": "^0.30.7",
    "supertest": "^6.2.4"
  }
}
